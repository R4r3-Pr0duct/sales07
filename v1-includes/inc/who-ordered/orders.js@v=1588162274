var iterations = 0;
var notification_interval = setInterval(show_notification, show_new_time);


function show_notification(){
    //var in_stock = parseInt($("#na_zalogi").text());
    //var dif_sel = 0;

    /*if(!in_stock){
        var dif_sel = 1;
        var in_stock = parseInt($("font").text());
    }*/

    var letters_to = "ABCDEFGHIJKLMNOPRSTUVZ";

    var name = buyers_data[language][start_with][0].split(" ")[0] + " " + buyers_data[language][start_with][0].split(" ")[1][0].toUpperCase() + ". ";
    var message =  '<img src="' + icon + '"><b>' + name + '</b>' + translations[language][0] + '<b>' + buyers_data[language][start_with][1] + '</b>' + translations[language][1];

    var wo_notification = '<div class="slide-top" id="wo_notification"><div class="slide">' + message + '</div></div>';
    $('body').append(wo_notification).find('#wo_notification').hide().fadeIn();
      
    setTimeout(function(){
        $('#wo_notification').fadeOut(function(){
            $(this).remove();
        });
    }, 5000);

    start_with++;

    if(start_with > 24){
        start_with = 0;
    }

    iterations++;

    if(iterations == 1){
        var timeout_val = 14000;
    }

    if(iterations == 2){
        var timeout_val = 20000;
    }

    if(iterations > 2){
        var timeout_val = 22000;
    }

    

    clearInterval(notification_interval);
    notification_interval = setInterval(show_notification, timeout_val);


    /*in_stock--;
    if(in_stock == 0){
        in_stock = 1;
    }
    $("#na_zalogi").text(in_stock);

    /*if(dif_sel == 1){
        $("font").text(in_stock)
    }*/
}

